{
	"name": "app",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"start": "node ./dist/server.js",
		"build": "rimraf dist && tsc",
		"ts.check": "tsc --project tsconfig.json",
		"add-build": "git add dist",
		"dev": "tsnd --transpile-only --respawn --ignore-watch node_modules src/server.ts"
	},
	"pre-commit": [
		"ts.check",
		"build",
		"add-build"
	],
	"dependencies": {
		"axios": "^1.6.2",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"https": "^1.0.0",
		"mssql": "^10.0.1",
		"oauth-signature": "^1.5.0",
		"reflect-metadata": "^0.2.0",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
		"typeorm": "^0.3.17",
		"typescript": "^5.3.3"
	},
	"devDependencies": {
		"@types/axios": "^0.14.0",
		"@types/express": "^4.17.21",
		"@types/node": "^20.10.4",
		"pre-commit": "^1.2.2",
		"rimraf": "^5.0.5"
	}
}
